{
	"name" : "conduction/digidbundle",
	"type" : "symfony-bundle",
	"description" : "Symfony Bundle for DigiD configuration",
	"keywords" : [
		"saml"
	],
	"homepage" : "https://conduction.nl",
	"license" : "MIT",
	"authors" : [{
			"name" : "Robert Zondervan",
			"email" : "robert@conduction.nl"
		}, {
			"name" : "Conduction",
			"homepage" : "https://conduction.nl"
		}
	],
	"require" : {
		"php" : "^7.4.0",
		"symfony/event-dispatcher" : "^5.0",
		"symfony/cache" : "^5.0@stable",
		"symfony/console" : "^5.0@stable",
		"symfony/security-bundle": "^5.3",
		"guzzlehttp/guzzle": "^6.3",
		"ramsey/uuid": "^3.8",
		"onelogin/php-saml": "^4.0",
		"conduction/commongroundbundle": "v2.*-dev | dev-feature-gateway",
		"conduction/samlbundle": "dev-master"
	},
	"autoload" : {
		"psr-4": {
			"Conduction\\DigidBundle\\": ""
		},
		"exclude-from-classmap" : [
			"/Tests/"
		]
	},
	"minimum-stability" : "alpha",
	"support" : {
		"email" : "ruben@conduction.nl",
		"issues" : "https://github.com/ConductionNL/digid-bundle/issues",
		"source" : "https://github.com/ConductionNL/digid-bundle"
	},
	"repositories" : [{
			"type" : "git",
			"url" : "https://github.com/ConductionNL/digid-bundle",
			"name" : "source"
		}
	]
}